Id: scm-avf2
	c1 <owl:allValuesFrom> y
	c1 <owl:onProperty> p1
	c2 <owl:allValuesFrom> y
	c2 <owl:onProperty> p2
	p1 <rdfs:subPropertyOf> p2         [Constraint p1 != p2 ]
